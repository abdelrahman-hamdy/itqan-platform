{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "agence104/livekit-server-sdk": "^1.3",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "google/apiclient": "^2.15",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.43",
        "laravel/pulse": "^1.5",
        "laravel/reverb": "^1.7.0",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^3.0",
        "munafio/chatify": "^1.6",
        "namu/wirechat": "^0.2.11",
        "nesbot/carbon": "^3.10",
        "opcodesio/log-viewer": "^3.21",
        "predis/predis": "^3.2",
        "resend/resend-laravel": "^1.1",
        "saade/filament-fullcalendar": "^4.0",
        "sentry/sentry-laravel": "^4.20",
        "setasign/fpdi": "^2.6",
        "shuvroroy/filament-spatie-laravel-health": "^3.0",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-health": "^1.34",
        "spatie/laravel-medialibrary": "^11.14",
        "spatie/laravel-multitenancy": "^4.0",
        "spatie/laravel-permission": "^6.0",
        "spatie/laravel-settings": "^3.3",
        "spatie/laravel-translatable": "^6.5",
        "tecnickcom/tcpdf": "^6.10",
        "ysfkaya/filament-phone-input": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "*",
        "beyondcode/laravel-query-detector": "*",
        "bottelet/translation-checker": "^2.0",
        "fakerphp/faker": "^1.23",
        "filament/upgrade": "^4.0",
        "larastan/larastan": "^3.8",
        "laravel/boost": "^1.0",
        "laravel/dusk": "^8.3",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "laravel/telescope": "^5.16",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "nunomaduro/phpinsights": "*",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-arch": "^3.1",
        "pestphp/pest-plugin-faker": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.2",
        "pestphp/pest-plugin-livewire": "^3.0",
        "pestphp/pest-plugin-type-coverage": "^3.6",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5.3",
        "spatie/pest-plugin-route-testing": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php",
            "app/Helpers/TimeHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "dev:full": [
            "Composer\\Config::disableProcessTimeout",
            "./dev-full.sh"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "munafio/chatify",
                "namu/wirechat"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
